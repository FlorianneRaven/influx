<head>
    <meta charset="utf-8">
    <meta name="referrer" content="no-referrer"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{% block title %}Installation de InFlux{% endblock title %}</title>
    {% block meta %}
        <meta name="viewport" content="width=device-width">
    {% endblock meta %}
    {% block stylesheets %}
        <link rel="stylesheet" href="/templates/{{ template_name }}/assets/css/style.css">
    {% endblock stylesheets %}
</head>
<body {% block body_class %}{% endblock body_class %}>
<div class="global-wrapper">
    <div id="header-container">
        <header class="wrapper clearfix">
            <h1 class="logo" id="title"><a href="/">In<i>Flux</i></a></h1>
            <nav>
            </nav>
        </header>
    </div>
    <div id="main-container">
        <div id="main" class="wrapper clearfix">
            <div id="menuBar">
                <aside>
                    <h3 class="left">{{ trans.INSTALL_PRE_REQUIS }}</h3>
                    <ul class="clear" style="margin:0">

                    </ul>
                </aside>
            </div>
        </div>
        <div class="container wrapper clearfix">
            {% block body %}{% endblock body %}
        </div>
    </div>
</div>

{% block javascripts %}
{% endblock javascripts %}
</body>
</html>